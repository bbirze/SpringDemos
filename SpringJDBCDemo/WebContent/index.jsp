<html>
  <body>
    <h3>Insert Segment</h3>
    <form action="InsertSegment" method="post">
      <table>
        <tr>
           <td>Flight Number:</td>
           <td><input type="text" name="flightNumber"></td>
        </tr>
        <tr>
           <td>Miles:</td>
           <td><input type="text" name="miles"></td>
        </tr>
        <tr>
           <td>Segment Date (yyyy-mm-dd):</td>
           <td><input type="text" name="segmentDate"></td>
        </tr>
        <tr>
           <td>Originating City:</td>
           <td><input type="text" name="originatingCity"></td>
        </tr>
        <tr>
          <td><input type="submit" value="Insert"></td>
        </tr>
      </table>
    </form>
  </body>
</html>